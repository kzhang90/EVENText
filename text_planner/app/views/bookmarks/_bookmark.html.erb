<div>
  <%= simple_form_for bookmark, :url => user_bookmarks_path(current_user, bookmark), :remote => true, :method => :post do |f| %>
  <%= f.hidden_field :title, { :value => bookmark["title"]} %>
  <%= f.hidden_field :image, { :value => bookmark["image"]} %>
  <%= f.hidden_field :description, { :value => bookmark["description"]} %>
  <%= f.hidden_field :date, { :value => bookmark["date"]} %>
  <%= f.hidden_field :time, { :value => bookmark["time"]} %>
  <%= f.hidden_field :url, { :value => bookmark["url"]} %>
  <div><%=bookmark.title%></div>
  <div><img src="<%=bookmark.image%>"></div>
  <div><%=bookmark.description%></div>
  <div><%=bookmark.date%></div>
  <div><%=bookmark.time%></div>
  <div><%=bookmark.url%></div>
  <%= f.button :submit class:"btn-primary"%>
  <% end %>
</div>

